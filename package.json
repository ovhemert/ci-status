  "license": "MIT",
  "engines": {
    "node": ">=8.9.0"
  },
  "devDependencies": {
    "sinon": "^6.3.4",
    "standard": "^12.0.1",
    "tap": "^12.0.1"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "columnify": "^1.5.4",
    "commander": "^2.18.0",
    "got": "^9.2.2",
    "xml2js": "^0.4.19"
  }
